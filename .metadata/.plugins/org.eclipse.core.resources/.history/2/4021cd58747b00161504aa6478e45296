package classwork8_Lists;

public abstract class Shape {
	String color;
	public Shape(){
		
	}
	
	public String getColor() {
		return color;
	}
	//public abstract void Junk();
	
}

class Square extends Shape implements ClosedShapeInterface {
	
	double side; 
	
	@Override
}


public interface ClosedShapeInterface {
	
	public double extension;
	
	public int ret;
}